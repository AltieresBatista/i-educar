{
    "name": "portabilis/i-educar",
    "description": "Software livre de gest√£o escolar",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "Portabilis",
        "i-Educar"
    ],
    "homepage": "https://github.com/portabilis/i-educar",
    "require": {
        "php": "^7.1.3",
        "cocur/slugify": "^3.1",
        "cossou/jasperphp": "^2.7",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.2",
        "honeybadger-io/honeybadger-laravel": "^1.4",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "robmorgan/phinx": "^0.10.6",
        "swiftmailer/swiftmailer": "^6.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/dusk": "^4.0",
        "mikey179/vfsStream": "^1.6",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/dbunit": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "ieducar/lib/App",
            "ieducar/lib/Utils"
        ],
        "psr-0": {
            "Core\\": "ieducar/lib/Core/",
            "CoreExt\\": "ieducar/lib/CoreExt/",
            "Portabilis\\": "ieducar/lib/Portabilis/"
        },
        "psr-4": {
            "App\\": "app/",
            "iEducar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "include-path": [
        "ieducar",
        "ieducar/intranet",
        "ieducar/lib",
        "ieducar/modules"
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "check-style": "vendor/bin/php-cs-fixer --diff --dry-run --verbose fix",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
